<Window x:Class="Magnit_app.Views.Windows.ReportSalesWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Magnit_app.Views.Windows"
        mc:Ignorable="d"
        Title="ReportSalesWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="70"/>
        </Grid.RowDefinitions>
        <WebBrowser Name="WebPrint" Margin="20" Grid.Row="0"/>
        <StackPanel Orientation="Horizontal" Grid.Row="1" HorizontalAlignment="Right">
            <Button Name="BtnPrint" Content="Печать" Click="BtnPrint_Click" Margin="10" FontFamily="Times New Roman" FontSize="20" Width="150" Height="40" HorizontalAlignment="Right" Cursor="Hand"/>
        </StackPanel>
    </Grid>
</Window>
